@model HBS.TinyMCE_Wysiwyg.TinyMCEEditorViewModel

@using (Html.Kentico().BeginInlineEditor("TinyMCEEditor", Model.PropertyName,
    new
    {
        @class = "TinyMCEEditor",
        data_enable_formatting = Model.EnableFormatting.ToString(),
        data_tools = Model.Tools,
        data_plugins = Model.Plugins

    }))
{
    @Html.Raw(Model.Html)
}
